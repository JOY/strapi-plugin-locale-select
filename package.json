    "styled-components": "^6.1.15",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
